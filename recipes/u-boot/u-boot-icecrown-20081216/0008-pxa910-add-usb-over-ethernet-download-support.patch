+void mv_usb_tr_complete_service(void*      handle, 
+                                 uint_8     type, 
+                                 boolean    setup,
+                                 uint_8     direction, 
+                                 uint_8_ptr buffer,
+                                 uint_32    length, 
+                                 uint_8     error)
+{
+    int                     dev_no = _usb_device_get_dev_num(handle);
+    struct mv_usb_dev       *mv_dev = &the_controller;
+    struct mv_usb_ep        *mv_ep;
